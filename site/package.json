{
	"name": "site",
	"version": "0.0.1",
	"description": "Site to test the Gatsby WooCommerce Theme",
	"main": "gatsby-config.js",
	"scripts": {
		"build": "gatsby build --log-pages",
		"build:clean": "npm run clean && npm run build",
		"dev": "gatsby develop",
		"dev:clean": "npm run clean && npm run dev",
		"clean": "gatsby clean",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"start": "npm run dev",
		"serve": "npm run clean && gatsby serve",
		"test": "jest",
		"test:debug": "jest --runInBand",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage=true",
		"test:update": "jest --verbose --updateSnapshot"
	},
	"author": "Imran Sayed",
	"license": "MIT",
	"dependencies": {
		"dotenv": "^8.2.0",
		"gatsby": "^2.24.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {}
}